{
  "name": "mana",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "dev:cloudflare": "wrangler dev",
    "build": "react-router build",
    "start": "wrangler dev",
    "deploy": "react-router build && wrangler pages deploy",
    "typecheck": "tsc",
    "db:init": "wrangler d1 execute mana-db --local --file=app/lib/db/schema.sql",
    "db:migrate": "wrangler d1 execute mana-db --file=app/lib/db/schema.sql"
  },
  "dependencies": {
    "@react-router/cloudflare": "^7.11.0",
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "isbot": "^5.1.17",
    "papaparse": "^5.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.11.0",
    "recharts": "^3.7.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260116.0",
    "@react-router/dev": "^7.11.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@types/xlsx": "^0.0.36",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.59.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
